{
  "database": {
    "target": {
      "database": "@declapract{variable.databaseName}",
      "schema": "@declapract{variable.databaseName}"
    },
    "role": {
      "cicd": {
        "username": "@declapract{variable.databaseUserName.cicdUser}",
        "password": "__PARAM__"
      },
      "crud": {
        "username": "@declapract{variable.databaseUserName.serviceUser}",
        "password": "__PARAM__"
      }
    },
    "tunnel": {
      "local": {
        "via": "jdbc",
        "host": "@declapract{variable.databaseTunnelHost.prod}",
        "port": 15433
      },
      "lambda": {
        "via": "rds-data-api",
        "resourceArn": "arn:aws:rds:us-east-1:@declapract{variable.awsAccountId.prod}:cluster:@declapract{variable.organizationName}db",
        "secretArn": "arn:aws:secretsmanager:us-east-1:@declapract{variable.awsAccountId.prod}:secret:rds-db-credentials/@declapract{variable.organizationName}db/user/@declapract{variable.databaseUserName.serviceUser}",
        "endpoint": null
      }
    }
  }
}
