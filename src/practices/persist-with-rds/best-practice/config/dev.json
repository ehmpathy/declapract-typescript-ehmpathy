{
  "database": {
    "target": {
      "database": "@declapract{variable.databaseName}",
      "schema": "@declapract{variable.databaseName}"
    },
    "role": {
      "cicd": {
        "username": "@declapract{variable.databaseUserName.cicdUser}",
        "password": "__CHANG3_ME__"
      },
      "crud": {
        "username": "@declapract{variable.databaseUserName.serviceUser}",
        "password": "__CHANG3_ME__"
      }
    },
    "tunnel": {
      "local": {
        "via": "jdbc",
        "host": "@declapract{variable.databaseTunnelHost.dev}",
        "port": 15432
      },
      "lambda": {
        "via": "rds-data-api",
        "resourceArn": "arn:aws:rds:us-east-1:@declapract{variable.awsAccountId.dev}:cluster:@declapract{variable.organizationName}db",
        "secretArn": "arn:aws:secretsmanager:us-east-1:@declapract{variable.awsAccountId.dev}:secret:rds-db-credentials/@declapract{variable.organizationName}db/user/@declapract{variable.databaseUserName.serviceUser}",
        "endpoint": null
      }
    }
  }
}
