{
  "database": {
    "target": {
      "database": "@declapract{variable.databaseName}",
      "schema": "@declapract{variable.databaseName}"
    },
    "role": {
      "cicd": {
        "username": "@declapract{variable.databaseUserName.cicdUser}",
        "password": "__CHANG3_ME__"
      },
      "crud": {
        "username": "@declapract{variable.databaseUserName.serviceUser}",
        "password": "__CHANG3_ME__"
      }
    },
    "tunnel": {
      "local": {
        "via": "jdbc",
        "host": "localhost",
        "port": 7821
      },
      "lambda": {
        "via": "rds-data-api",
        "resourceArn": "arn:aws:rds:us-east-1:000000000000:cluster:@declapract{variable.databaseName}-local",
        "secretArn": "arn:aws:secretsmanager:us-east-1:000000000000:secret:@declapract{variable.databaseName}-local/@declapract{variable.databaseUserName.serviceUser}",
        "endpoint": "http://localhost:7823"
      }
    }
  }
}
